{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "crons": [{
      "path": "/api/cron",
      "schedule": "0 0 * * *"
    }]
  }